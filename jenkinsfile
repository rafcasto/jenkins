pipeline {
    agent any

    stages {
    stage('Test') {
                steps {
                    sh './mvnw test'
                }
            }
        stage('Code coverage') {
            steps {
                sh './mvnw jacoco:report'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}