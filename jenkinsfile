pipeline {
    agent any

    stages {
     stage('Verify Code Coverage') {
                steps {
                    echo './mvnw clean verify'
                }
            }
    stage('Test') {
                steps {
                    sh './mvnw test'
                }
            }
        stage('Code coverage') {
            steps {
                sh './mvnw jacoco:report'
            }
        }


    }
}